define fib = int lambda n:int . if (n<=2) then 1 else fib(n-1)+fib(n-2);
print(fib(10));
define x = 0;
if (x>0 && 10/x>1) then { print("\nA\n"); } else { print("\nB\n"); }
if (x<1 || x<fib(100)) then { print("C\n"); } else { print("D\n"); }


